{
  "name": "package.json",
  "version": "1.0.0",
  "description": "API for the LiveLot application",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.js",
    "test": "mongo LiveLotAPI_TEST --eval \"db.dropDatabase()\" && MONGODB_URI=\"mongodb://127.0.0.1:27017/LiveLotAPI_TEST\" nodemon --watch . --ext ts --exec \"mocha -r ts-node/register ./src/test/*.test.ts\"",
    "start": "node ./built/server.js",
    "local": "nodemon server.js",
    "docker": "tsc && docker-compose up"
  },
  "keywords": [
    "heroku"
  ],
  "author": "Madison Rogers",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "^5.7.14",
    "@types/socket.io": "^2.1.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2",
    "socket.io": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "10.x"
  }
}
